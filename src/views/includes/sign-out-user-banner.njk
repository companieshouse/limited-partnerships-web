{% if userEmail %}

  {% set signOutPath = 'sign-out?page=' + templateName %}

  <ul id="navigation" class="sign-out-user-banner">
    <li class="content-email">{{ userEmail }}</li>
    <li>
      <a class="govuk-link" href="{{ signOutBaseUrl }}{{ signOutPath }}" data-event-id="signout">Sign out</a>
    </li>
  </ul>

{% endif %}
