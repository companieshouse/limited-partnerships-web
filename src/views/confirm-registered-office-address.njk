{% extends "layout.njk" %}

{% block content %}
  <div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
      <h1 class="govuk-heading-xl">
        Confirm the registered office address
      </h1>
      {{ props.data.cache.registration_registered_office_address.premises }}<br>
      {{ props.data.cache.registration_registered_office_address.address_line_1 }}<br>
      {{ props.data.cache.registration_registered_office_address.address_line_2 }}<br>
      {{ props.data.cache.registration_registered_office_address.locality }}<br>
      {{ props.data.cache.registration_registered_office_address.region }}<br>
      {{ props.data.cache.registration_registered_office_address.country }}<br>
      {{ props.data.cache.registration_registered_office_address.postal_code }}<br>
    </div>
  </div>
{% endblock %}
