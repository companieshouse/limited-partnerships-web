{% extends "layout.njk" %}

{% set pageTitle = i18n.transactionFiled.title %}

{% block content %}
    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds">
      
            <h1 class="govuk-heading-xl">{{ i18n.transactionFiled.title }}</h1> 

            <p class="govuk-body">{{ i18n.transactionFiled.alreadyReceived }} {{ props.data.limitedPartnership.data.partnership_name }} ({{ props.data.limitedPartnership.data.partnership_number }}). </p>

            <p class="govuk-body">{{ i18n.transactionFiled.acceptedInformation | safe }}</p>
  
        </div>
    </div>
{% endblock %}